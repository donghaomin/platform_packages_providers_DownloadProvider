<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Action buttons in download notification -->
    <!-- 下载通知中的操作按钮 -->
    <string name="download_stop">Stop</string>
    <string name="download_resume">Resume</string>
    <string name="download_pause">Pause</string>
    <string name="download_retry">Retry</string>

    <string name="download_stop_all">Stop all</string>
    <string name="download_resume_all">Resume all</string>
    <string name="download_pause_all">Pause all</string>
    <string name="download_retry_all">Retry all</string>

    <!-- Time duration in seconds -->
    <!-- 以秒钟表达的时长 -->
    <plurals name="duration_seconds">
         <item quantity="one"><xliff:g id="count">%d</xliff:g> sec</item>
         <item quantity="other"><xliff:g id="count">%d</xliff:g> secs</item>
    </plurals>
    <!-- Time duration in minutes -->
    <!-- 以分钟表达的时长 -->
    <plurals name="duration_minutes">
         <item quantity="one"><xliff:g id="count">%d</xliff:g> min</item>
         <item quantity="other"><xliff:g id="count">%d</xliff:g> mins</item>
    </plurals>
    <!-- Time duration in hours -->
    <!-- 以小时表达的时长 -->
    <plurals name="duration_hours">
         <item quantity="one"><xliff:g id="count">%d</xliff:g> hr</item>
         <item quantity="other"><xliff:g id="count">%d</xliff:g> hrs</item>
    </plurals>

</resources>
