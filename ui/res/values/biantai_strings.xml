<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Paused download status text -->
    <!-- 暂停下载状态文字 -->
    <string name="download_pause">Paused</string>

    <!-- Download progress percentage -->
    <!-- 下载进度百分比 -->
    <string name="download_percent"><xliff:g id="number">%d</xliff:g><xliff:g id="percent">%%</xliff:g></string>

    <!-- Resume or pause download dialog message text-->
    <!-- 继续或暂停下载对话框信息文字 -->
    <string name="dialog_title">Download</string>
    <string name="paused_dialog_msg">Do you want to pause download?</string>
    <string name="resume_dialog_msg">Do you want to resume download?</string>

</resources>
